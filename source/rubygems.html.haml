.container
  .row
    .col-sm-12
      = partial :page_header
      - data.rubygems.each do |gem|
        .rubygem
          %h3= gem.title
          %p= gem.description
          %ul.list-inline
            -  if gem.github
              %li= link_to 'Github', gem.github
            - if gem.rubygems
              %li= link_to 'Rubygems', gem.rubygems
